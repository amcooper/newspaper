<%= form_for @article do |f| %>
<%= f.label :headline %>
<%= f.text_field :headline %>
<br />
<%= f.label :byline %>
<%= f.text_field :byline %>
<br />
<%= f.label :photo_url %>
<%= f.text_field :photo_url %>
<br />
<%= f.label :content %>
<%= f.text_area :content %>
<br />
<%= f.submit %>
<% end %>